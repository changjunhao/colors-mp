/**index.wxss**/
.container {
    background-image: url(https://m.ifable.cn/images/bg.png), url(data:image/png;base64,);
    background-size: auto;
    margin: 0 auto;
    background-repeat: repeat, repeat-x;
    background-position: 0 0;
    overflow: hidden;
}
.color-view {
    display: flex;
    justify-content: space-around;
    align-items: center;
    box-shadow: rgba(0, 0, 0, 0.5) 0 0 8px;
    position: relative;
    width: 600rpx;
    height: 114rpx;
    border-radius: 6px;
    padding: 26rpx 13rpx;
    margin: 0 auto;
}
.color-name {
    font-size: 100rpx;
    font-family: TChinese, serif;
    letter-spacing: -12rpx;
}
.color-pinyin {
    text-transform: capitalize;
    font-size: 24rpx;
    position: absolute;
    left: 10rpx;
    top: 10rpx;
}
.color-view image {
    width: 114rpx;
    height: 114rpx;
    position: absolute;
    right: 0;
}
